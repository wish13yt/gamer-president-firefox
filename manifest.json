{
  "name": "Gamer President",
  "version": "0.1.0",
  "manifest_version": 3,
  "description": "Replaces President with Gamer President for your lovely web experience. Manifest 2 version by dhtdht020 on GitHub.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
